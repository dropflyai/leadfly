    {
      "parameters": {
        "functionCode": "// Create daily statistics report\nconst stats = $json.statistics;\nconst cleanup = $previousNode.json;\n\nconst report = {\n  type: 'daily_task_report',\n  title: 'ðŸ“Š Daily Task Processing Report',\n  data: {\n    period: 'last_7_days',\n    total_tasks: stats?.total_week || 0,\n    success_rate: stats?.success_rate || 0,\n    task_breakdown: stats?.by_type || {},\n    status_breakdown: stats?.by_status || {},\n    cleanup_summary: {\n      completed_tasks_removed: cleanup?.deleted_count || 0\n    },\n    system_health: stats?.success_rate >= 95 ? 'excellent' : \n                   stats?.success_rate >= 90 ? 'good' : \n                   stats?.success_rate >= 80 ? 'fair' : 'poor'\n  },\n  timestamp: new Date().toISOString()\n};\n\nreturn { report };"
      },
